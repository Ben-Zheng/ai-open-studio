load("@io_bazel_rules_go//go:def.bzl", "go_library")

go_library(
    name = "go_default_library",
    srcs = ["site.go"],
    importpath = "go.megvii-inc.com/brain/brainpp/projects/aiservice/resourcesense/pkg/api/v1/site",
    visibility = ["//visibility:public"],
    deps = [
        "//projects/aiservice/pkg/ginlib:go_default_library",
        "//projects/aiservice/resourcesense/pkg/controller/site:go_default_library",
        "//projects/aiservice/resourcesense/pkg/types/ctx:go_default_library",
        "//projects/aiservice/resourcesense/pkg/types/site:go_default_library",
        "@com_github_gin_gonic_gin//:go_default_library",
